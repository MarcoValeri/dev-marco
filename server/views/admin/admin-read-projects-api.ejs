<%- include('./../includes/head.ejs'); %>
        <title><%= pageTitle %></title>
    </head>
    <body>
        <h1>Admin Read Projects API</h1>
        <%- include('../includes/navigation.ejs'); %>
        <h2>Projects API data</h2>
        <% projectsApi.projects.forEach(project => { %>
            <p>Project ID: <%= project.projectId %></p>
            <p>Project Image: <%= project.projectImage %></p>
            <p>Project Title: <%= project.projectTitle %></p>
            <p>Project Description: <%= project.projectDescription %></p>
            <p>Project Show Repository: <%= project.showRepository %></p>
            <p>Project Repository: <%= project.projectRepository %></p>
            <p>Project URL: <%= project.projectUrl %></p>
            <p>Project Button: <%= project.projectButton %></p>
            <br />
        <% }) %>
    </body>
</html>
